CMD:kezetfog(playerid,params[])
{
	if(isnull(params))
	{
		SCM(playerid,COL_MKEK,"Használat: /kezetfog [Típus] [Játékos Neve / ID]");
		return 1;
	}
	new t,p;
	sscanf(params,"du",t,p);
	if(t < 1 || t > 3)
	{
		SCM(playerid,COL_LRED,"1-3 között!!");
