			break;
		}
	}
	if(id != NINCS)
	{
		switch( id )
		{
			case BIZ_PB:
			{
				Freeze(playerid, 2000);
				PaintBallMuvelet(playerid, 2);
				SetPos(playerid, PosExt(CegInfo[id][cPosBe]), CegInfo[id][cPosBe][3]);
				SetVirtualWorld(playerid, CegInfo[id][cVw][0]);
				SetInterior(playerid, CegInfo[id][cInt][0]);
