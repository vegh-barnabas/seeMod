			}
		}
	}
	if(killerid != INVALID_PLAYER_ID && !PlayerInfo[killerid][pAduty] && !PlayerInfo[killerid][pHarcol] && !PlayerInfo[killerid][pMaszkVisel] && !IsHitman(playerid))
	{
