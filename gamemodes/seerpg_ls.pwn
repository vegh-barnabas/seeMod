	   		}
			return true;
		}
	}

	i = NINCS;
	for(;++i < MAX_CEG;)
	{
		if(CegInfo[i][cVan] && CegInfo[i][cPickup] == pickupid)
		{
			new str[128];
			if(CegInfo[i][cTulajID] != NINCS)
				format(str, 128, "~w~%s~w~~n~Tulaj: %s~n~Belépõ: ~g~%s$~n~Belépézhez: /enter", CegInfo[i][cNev], CegInfo[i][cTulaj], FN(CegInfo[i][cBelepo], 0, ','));
