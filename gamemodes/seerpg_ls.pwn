				nformat(str, 128, "<< %s leállította a tûz akciót! (Pénz nem kerül levonásra) >>", JatekosNev(idx));
				FrakcioUzenet(COLOR_YELLOW, FRAKCIO_LAFD, str);
			}
		}
	}
	return true;
}

