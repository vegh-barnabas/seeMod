stock Time(const mikell[])
{
	new
		masodperc, perc, ora,
		nap, honap, ev;

	new ido;

	gettime(ora, perc, masodperc);
	getdate(ev, honap, nap);

	if(!strcmp("mp", mikell))
		ido = masodperc;
	if(!strcmp("perc", mikell))
		ido = perc;
	if(!strcmp("ora", mikell))
		ido = ora;
	if(!strcmp("nap", mikell))
		ido = nap;
	if(!strcmp("honap", mikell))
		ido = honap;
	if(!strcmp("ev", mikell))
		ido = ev;
	return ido;
}

