	if(!OOC_Ellenorzes(playerid, "/d", params))
		return true;

	if(fk == FRAKCIO_LSFD)
		format(prox, sizeof(prox),"** Tûzoltó %s %s: %s ,vége ", Rangok(PlayerInfo[playerid][prang], PlayerInfo[playerid][pFrakcio]), JatekosNev(playerid, true), params);
