	new date[3],time[3];
	getdate(date[0], date[1], date[2]);
	gettime(time[0], time[1], time[2]);
	doQuery("INSERT INTO `"#MYSQL_LOG_TABLA"`(nev, datum, tipus) VALUES('%s', '%d-%02d-%02d %02d:%02d', 'Kick')", JatekosNev(playerid), PosExt(date), time[0], time[1]);


	Kick(player);
 	return true;
}


