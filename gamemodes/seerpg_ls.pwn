				   }
				}
				GetPlayer2DZone(playerid, zona, MAX_ZONE_NAME);
				format(str1, sizeof(str1), "[CCTV]: {1E90FF}%s körzetben "COL_LRED"gyilkosság"COL_LKEK" történt! Jelölve a térképen)",zona);
