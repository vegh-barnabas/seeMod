	}
	SendClientMessage(jatekos, 0xffff00AA, uzenet);
	ProxDetector(15.0, playerid, "Valakik összesúgtak...", COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, false);
