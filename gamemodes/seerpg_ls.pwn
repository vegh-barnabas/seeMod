		new dstr[1024], tmp[128], i = NINCS;
		for(;++i < sizeof(Katona_Engedelyek);)
		{
			if(Katona_Engedelyek[i][1][0]) format(tmp, sizeof(tmp), "\n"#COL_MKEK"%s",Katona_Engedelyek[i][0]);
