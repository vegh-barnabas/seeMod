	else if(!strcmp(params, "felvesz", true))
	{
		new dstr[1024], tmp[128], i = NINCS;
		for(;++i < sizeof(FBI_Engedely);)
		{
			if(FBI_Engedely[i][1][0]) format(tmp, sizeof(tmp), "\n"#COL_MKEK"%s",FBI_Engedely[i][0]);
